<script>
$('#moveNorth').click(() => {
  $.ajax({
    url: `/moveNorth`,
    type: "GET",
    success: res => {
      $("#gameLog").append(JSON.stringify(res));
      $("#gameLog").append('\n\n');
    }
  });
});
  
