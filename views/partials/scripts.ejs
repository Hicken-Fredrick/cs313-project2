<script>
$('#moveNorth').click(() => {
  $("#gameLog").append('CALLED MOVE NORTH \n');
  $.ajax({
    url: `/moveNorth`,
    type: "GET",
    success: res => {
      $("#gameLog").append(JSON.stringify(res));
      $("#gameLog").append('\n');
    }
  });
});
  
